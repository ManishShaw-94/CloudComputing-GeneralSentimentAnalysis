{
  "name": "twitter-data-analysis",
  "version": "1.0.0",
  "description": "This web app is an attempt to visualise sentiment score of the Australian population for 9 of their politicians correlated against 3 factors: the economy size of an area, the agr group distribution within an area, and the population diversity of an area. This web app implements APIs to extract data from CouchDB, preprocesses it before visualising it using React.js.",
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-client": "webpack --mode=development --config webpack.config.js --watch",
    "dev:build-server": "webpack --mode=development --config webpack.server.config.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "prod": "webpack --mode=production"
  },
  "author": "Abhishek Ugrasen Yadav (student id: 1176058)",
